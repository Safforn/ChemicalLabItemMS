js/plugins/slimscroll/jquery.slimscroll.min.js\"></script>\r\n<script src=\"js/plugins/jeditable/jquery.jeditable.js\"></script>\r\n\r\n<!-- Data Tables -->\r\n<script src=\"js/plugins/dataTables/jquery.dataTables.js\"></script>\r\n<script src=\"js/plugins/dataTables/dataTables.bootstrap.js\"></script>\r\n\r\n<!-- Custom and plugin javascript -->\r\n<script src=\"js/hplus.js?v=2.2.0\"></script>\r\n<script src=\"js/plugins/pace/pace.min.js\"></script>\r\n\r\n<!-- iCheck -->\r\n<script src=\"js/plugins/iCheck/icheck.min.js\"></script>\r\n<script>\r\n    //领用用途校验\r\n    let purposeElement = document.getElementById(\"purpose\");\r\n    purposeElement.onblur = checkPurpose;\r\n    function checkPurpose() {\r\n        let purpose = purposeElement.value.trim();\r\n        var purpose_reg = /^[\\s\\S]*.*[^\\s][\\s\\S]*$/;\r\n        var flag = purpose_reg.test(purpose);\r\n        if (flag) {\r\n            //账号合法\r\n            purposeElement.style.border = \"\";\r\n        } else {\r\n            //账号非法,加一个红色边框\r\n            purposeElement.style.border = \"1px solid red\";\r\n        }\r\n        return flag;\r\n    }\r\n\r\n    //开始日期校验\r\n    let startElement = document.getElementById(\"start_time\");\r\n    startElement.onblur = checkStart;\r\n    function checkStart() {\r\n        var today = new Date();\r\n        var dd = today.getDate();\r\n        var mm = today.getMonth() + 1; //January is 0!\r\n        var yyyy = today.getFullYear();\r\n        if (dd < 10) {dd = '0' + dd;}\r\n        if (mm < 10) {mm = '0' + mm;}\r\n        var today = yyyy + '-' + mm + '-' + dd;\r\n        //alert(today);\r\n        let start_time = startElement.value.trim();\r\n        //alert(start_time);\r\n        var start_time_reg =/^[\\s\\S]*.*[^\\s][\\s\\S]*$/;\r\n        var flag = start_time_reg.test(start_time);\r\n        if (flag&&start_time>=today) {\r\n            //账号合法\r\n            startElement.style.border = \"\";\r\n        } else {\r\n            //账号非法,加一个红色边框\r\n            startElement.style.border = \"1px solid red\";\r\n        }\r\n        return flag;\r\n    }\r\n\r\n    //结束日期校验\r\n    let endElement = document.getElementById(\"end_time\");\r\n    endElement.onblur = checkEnd;\r\n    function checkEnd() {\r\n        let start_time = startElement.value.trim();\r\n        let end_time = endElement.value.trim();\r\n        // var end_time_reg =/^[\\s\\S]*.*[^\\s][\\s\\S]*$/;\r\n        // var flag = end_time_reg.test(end_time);\r\n        if (start_time<=end_time) {\r\n            //账号合法\r\n            endElement.style.border = \"\";\r\n        } else {\r\n            //账号非法,加一个红色边框\r\n            endElement.style.border = \"1px solid red\";\r\n        }\r\n        return flag;\r\n    }\r\n\r\n\r\n</script>\r\n<!-- 导入left_sidebar -->\r\n<script>\r\n    $(function () {\r\n        $.get(\"left_sidebar.html\",function (data) {\r\n            $(\"#left_sidebar\").html(data);\r\n        });\r\n\r\n        use_checked();\r\n\r\n        $(\"#btn-save-requisition\").click(function () {\r\n            console.log($(\"#orderForm\").serialize())\r\n            console.log($(\"#editable\").serialize())\r\n        });\r\n    });\r\n\r\n    function use_checked() {\r\n        $(\"#end_time\").attr(\"disabled\", true)\r\n    }\r\n    function borrow_checked() {\r\n        $(\"#end_time\").attr(\"disabled\", false)\r\n    }\r\n\r\n\r\n    $(document).ready(function () {\r\n        $('.i-checks').iCheck({\r\n            checkboxClass: 'icheckbox_square-green',\r\n            radioClass: 'iradio_square-green',\r\n        });\r\n    });\r\n<!-- Page-Level Scripts -->\r\n    $(document).ready(function () {\r\n        $('.dataTables-example').dataTable();\r\n\r\n        /* Init DataTables */\r\n        let oTable = $('#editable').dataTable();\r\n\r\n        /* Apply the jEditable handlers to the table */\r\n        oTable.$('td').editable('../example_ajax.php', {\r\n            \"callback\": function (sValue, y) {\r\n                let aPos = oTable.fnGetPosition(this);\r\n                oTable.fnUpdate(sValue, aPos[0], aPos[1]);\r\n            },\r\n            \"submitdata\": function (value, settings) {\r\n                return {\r\n                    \"row_id\": this.parentNode.getAttribute('id'),\r\n                    \"column\": oTable.fnGetPosition(this)[2]\r\n                };\r\n            },\r\n\r\n            \"width\": \"90%\",\r\n            \"height\": \"100%\"\r\n        });\r\n\r\n\r\n    });\r\n\r\n    // 让 点击的该行 可编辑\r\n    function fnClickAddRow() {\r\n        $('#editable').dataTable().fnAddData([\r\n            \"New ID\",\r\n            \"New Name\",\r\n            \"New num\",\r\n            \"New unit\",  // 单位\r\n            \"New notes\"]);\r\n    }\r\n</script>\r\n\r\n\r\n</body>\r\n\r\n</html>\r\n
