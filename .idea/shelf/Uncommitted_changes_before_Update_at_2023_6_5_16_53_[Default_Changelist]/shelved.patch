+        var end_time_reg =/^[\s\S]*.*[^\s][\s\S]*$/;
+        var flag = end_time_reg.test(end_time);
+        if (flag&&start_time<=end_time) {
+            //账号合法
+            endElement.style.border = "";
+        } else {
+            //账号非法,加一个红色边框
+            endElement.style.border = "1px solid red";
+        }
+        return flag;
+    }
+
