+        var flag = purpose_reg.test(purpose);
+        if (flag) {
+            //账号合法
+            purposeElement.style.border = "";
+        } else {
+            //账号非法,加一个红色边框
+            purposeElement.style.border = "1px solid red";
+        }
+        return flag;
+    }
 
+    //开始日期校验
+    let startElement = document.getElementById("start_time");
+    startElement.onblur = checkStart;
+    function checkStart() {
+        var today = new Date();
+        var dd = today.getDate();
+        var mm = today.getMonth() + 1; //January is 0!
+        var yyyy = today.getFullYear();
+        if (dd < 10) {dd = '0' + dd;}
+        if (mm < 10) {mm = '0' + mm;}
+        var today = yyyy + '-' + mm + '-' + dd;
+        //alert(today);
+        let start_time = startElement.value.trim();
+        //alert(start_time);
+        var start_time_reg =/^[\s\S]*.*[^\s][\s\S]*$/;
+        var flag = start_time_reg.test(start_time);
+        if (flag&&start_time>=today) {
+            //账号合法
+            startElement.style.border = "";
+        } else {
+            //账号非法,加一个红色边框
+            startElement.style.border = "1px solid red";
+        }
+        return flag;
+    }
 
+    //结束日期校验
+    let endElement = document.getElementById("end_time");
