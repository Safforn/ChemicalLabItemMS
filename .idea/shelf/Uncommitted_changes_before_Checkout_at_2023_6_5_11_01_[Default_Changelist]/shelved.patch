.val(data_json[index].location);\r\n        $(\"#update_rentorbuy\").val(data_json[index].rentorbuy);\r\n\r\n        //弹出修改表单（非模态框）\r\n        $('#updateModal').modal('show')\r\n    }\r\n\r\n    //查看货物详情弹窗\r\n    function ViewById(row, index){\r\n        var data = JSON.stringify($(\"#myTable\").bootstrapTable('getData')); //获取全部数据\r\n        var data_json = JSON.parse(data); //data_json和data_json2一样都是json对象\r\n        console.log(data_json);\r\n        $(\"#check_gid\").val(data_json[index].gid);\r\n        $(\"#check_name\").val(data_json[index].name);\r\n        $(\"#check_type\").val(data_json[index].type);\r\n        $(\"#check_price\").val(data_json[index].price);\r\n        $(\"#check_picture\").val(data_json[index].picture);\r\n        $(\"#check_num\").val(data_json[index].num);\r\n        $(\"#check_description\").val(data_json[index].description);\r\n        $(\"#check_location\").val(data_json[index].location);\r\n        $(\"#check_rentorbuy\").val(data_json[index].rentorbuy);\r\n\r\n        //弹出修改表单（非模态框）\r\n        $('#checkModal').modal('show');\r\n    }\r\n\r\n    //点击操作的x按钮删除当前行的数据\r\n    function DeleteById(row, index){\r\n        var data = JSON.stringify($(\"#myTable\").bootstrapTable('getData')); //获取全部数据\r\n        var data_json = JSON.parse(data); //data_json和data_json2一样都是json对象\r\n        //console.log(data_json);\r\n        var gid = data_json[index].gid;\r\n        var obj = $(\"#myTable\");\r\n        var ids = $.map(obj.bootstrapTable('getSelections'), function (row) {\r\n            return row.ID;\r\n        });\r\n        //obj.bootstrapTable('removeByUniqueId', gid);\r\n        obj.bootstrapTable('remove', { field:'gid', values: gid });  //删除一行\r\n\r\n        $.post(\"admin/deleteGoods\", {gid : gid}, function () {  //通知后端删除数据\r\n            load();  // 重新加载表格页面\r\n        });\r\n    }\r\n\r\n\r\n    // 入口函数\r\n    $(function () {\r\n\r\n        load();\r\n\r\n        // 新增货物 提交按钮\r\n        var submitWork = document.getElementById(\