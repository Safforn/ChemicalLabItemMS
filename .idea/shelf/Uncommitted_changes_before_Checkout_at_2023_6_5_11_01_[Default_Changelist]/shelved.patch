mary\" data-toggle=\"modal\" data-target=\"#myModal\"\r\n                        style=\"background-color: #ff884a; border-color: #f4e0d0\">新增货物</button>\r\n            </div>\r\n\r\n        <!-- 新增数据表单弹窗 -->\r\n        <form action=\"admin.html\" method=\"post\" class=\"form-horizontal\" role=\"form\" id=\"addGoodsForm\">\r\n            <div class=\"modal fade\" id=\"myModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"myModalLabel\" aria-hidden=\"true\">\r\n                <div class=\"modal-dialog\">\r\n                    <div class=\"modal-content\">\r\n                        <div class=\"modal-header\">\r\n                            <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-hidden=\"true\">\r\n                                ×\r\n                            </button>\r\n                            <h4 class=\"modal-title\" id=\"myModalLabel\">\r\n                                新增货物\r\n                            </h4>\r\n                        </div>\r\n                        <div class=\"modal-body\" style=\"height: 100%;\">\r\n\r\n                            <div class=\"form-group\">\r\n                                <label for=\"gid\" class=\"col-sm-3 control-label\">货物编号</label>\r\n                                <div class=\"col-sm-7\">\r\n                                    <input type=\"text\" class=\"form-control\" name=\"gid\" id=\"gid\"\r\n                                           placeholder=\"请输入货物编号\">\r\n                                </div>\r\n                            </div>\r\n\r\n                            <div class=\"form-group\">\r\n                                <label for=\"name\" class=\"col-sm-3 control-label\">货物名称</label>\r\n                                <div class=\"col-sm-7\">\r\n                                    <input type=\"text\" class=\"form-control\" name=\"name\" id=\"name\"\r\n                                           placeholder=\"请输入货物名称\">\r\n                            