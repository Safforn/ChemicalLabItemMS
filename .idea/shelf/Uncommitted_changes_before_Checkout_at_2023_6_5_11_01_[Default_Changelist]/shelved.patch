                    //查询框中的参数传递给后台\r\n                    search_kw: $('#search-keyword').val(), // 请求时向服务端传递的参数\r\n                };\r\n                return query_params;\r\n            },\r\n            columns: [\r\n                {\r\n                    checkbox:true  //第一列显示复选框\r\n                }, {\r\n                    field: 'gid',  //返回数据rows数组中的每个字典的键名与此处的field值要保持一致\r\n                    title: '编号',\r\n                    align: 'center',\r\n                    valign: 'middle'\r\n                }, {\r\n                    field: 'name',\r\n                    title: '名称',\r\n                    align: 'center',\r\n                    valign: 'middle'\r\n                }, {\r\n                    field: 'type',\r\n                    title: '类型',\r\n                    align: 'center',\r\n                    valign: 'middle'\r\n                }, {\r\n                    field: 'price',\r\n                    title: '价格',\r\n                    align: 'center',\r\n                    valign: 'middle'\r\n                }, {\r\n                    field: 'picture',\r\n                    title: '图片地址',\r\n                    align: 'center',\r\n                    valign: 'middle'\r\n                },{\r\n                    field: 'num',\r\n                    title: '数量',\r\n                    align: 'center',\r\n                    valign: 'middle'\r\n                }, {\r\n                    field: 'description',\r\n                    title: '货物说明',\r\n                    align: 'center',\r\n                    valign: 'middle'\r\n                }, {\r\n                    field: 'location',\r\n                    title: '地点',\r\n                    align: 'center',\r\n                    valign: 'middle'\r\n                }, {\r\n                    field: 'rentorbuy',\r\n                    title: '租赁或购买',\r\n                    align: 'center',\r\n                    valign: 'middle'\r\n                }, {\r\n                    